{
	"name": "resume-builder",
	"version": "1.1.0",
	"description": "Resume Builder Application using ReactJS(frontend) and NodeJS(backend)",
	"main": "server.js",
	"scripts": {
		"client-install": "npm install --prefix client",
		"start": "node server.js",
		"server": "nodemon server.js",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"prod": "concurrently \"npm run start\" \"npm run client\""
	},
	"author": "Aayush Bharti",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.20.2",
		"concurrently": "^8.2.2",
		"cors": "^2.8.5",
		"express": "^4.19.2",
		"html-pdf": "^3.0.1"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.19",
		"nodemon": "^3.1.0",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.3"
	}
}
